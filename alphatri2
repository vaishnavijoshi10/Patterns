#include <iostream>
using namespace std;

int main()
{
   int n,i,j,count;
   cin>> n;
   char ch;
  
   i=1;
  
    ch='A';
   while(i<=n){
     j=1;
    
       while(j<=i){
          cout<<ch;
          ch++;
          j++;
       }
       cout<<endl;
       
       
       i++;
   }
    return 0;
}
