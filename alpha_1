#applegit
#include <iostream>
using namespace std;

int main()
{
   int n,i,j,count;
   cin>> n;
   char ch;
   ch='A';
   i=1;
  
   
   while(i<=n){
     j=1;
     
       while(j<=n){
          cout<<ch;
         
          j++;
       }
       cout<<endl;
       ch++;
      
       i++;
   }
    return 0;
}
